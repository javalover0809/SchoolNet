<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.TTEnglish.backend.dao.ContentMapper">
    <insert id="inputContent">
    insert into publish_content(username,title,content) value('tangguojie',#{1},#{2})
    </insert>
    <select id="selectContent" resultType="com.TTEnglish.backend.model.Content">
        select * from publish_content where id = 16
    </select>
</mapper>

