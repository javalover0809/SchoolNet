<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.TTEnglish.backend.dao.Content">
<!-- 1 resultType：返回值类型,值为对应实体类的类路径
     2 id:对应类的方法，使得接口和配置文件（具体到方法）绑定
-->
<select id="setContent"  resultType="com.TTEnglish.backend.model.Content">
    insert into publish_content(username,content) value('tangguojie',#{content})
</select>
</mapper>

